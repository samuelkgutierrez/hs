app (
    fix (
        abs (f:((Nat -> Nat) -> Bool) .
            abs (x:(Nat -> Bool) .
                abs (y:Nat .
                    if iszero x
                        then False
                    else
                        if iszero y
                            then True
                        else app (f, app (x, y))
                        fi
                    fi
            )
        )
    ),
    succ succ succ 0
)
